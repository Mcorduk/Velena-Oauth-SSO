{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "paths": {
      "*": ["./node_modules/*"],
    },
  },
  "include": ["src/**/**/*.ts", "config/config.ts", "src/utils/hashPass.js"],
  "exclude": ["node_modules", "dist"],
}
